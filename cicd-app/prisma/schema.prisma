datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model City {
 id Int @id @default(autoincrement())
 department_code String 
 insee_code String?
 zip_code String?
 name String
 lat Float
 lon Float

 @@map("city")
}
